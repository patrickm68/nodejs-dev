{
  "name": "instana-nodejs-sensor",
  "version": "1.8.0",
  "description": "The Instana Node.js sensor",
  "main": "src/index.js",
  "scripts": {
    "test": "mocha --reporter spec $(find src -name '*_test.js') && eslint src",
    "dev": "node -e 'require(\".\")()' | bunyan",
    "dev-http": "node test/instrumentation/http/index.js | bunyan",
    "dev-http-to-dummy-agent": "AGENT_PORT=3210 npm run dev-http",
    "dummy-agent": "AGENT_PORT=3210 node test/dummyAgent.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/instana/nodejs-sensor.git"
  },
  "keywords": [
    "instana",
    "agent",
    "sensor",
    "monitoring"
  ],
  "author": "Ben Ripkens <ben@instana.com>",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/instana/nodejs-sensor/issues"
  },
  "homepage": "https://github.com/instana/nodejs-sensor#readme",
  "dependencies": {
    "bunyan": "^1.5.1",
    "event-loop-lag": "1.1.0"
  },
  "optionalDependencies": {
    "event-loop-stats": "1.0.0",
    "gcstats.js": "1.0.0"
  },
  "devDependencies": {
    "body-parser": "1.15.2",
    "chai": "3.3.0",
    "cz-conventional-changelog": "1.1.5",
    "eslint": "1.7.2",
    "eslint-config-airbnb": "0.1.0",
    "express": "4.14.0",
    "mocha": "2.3.3",
    "proxyquire": "1.7.3",
    "sinon": "1.17.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
